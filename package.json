{
  "name": "pacto",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "clean:dist": "rimraf dist",
    "clean:docs": "rimraf docs",
    "build:prod": "ng build --configuration production",
    "build:prod:gh": "ng build --configuration production --base-href \"/pacto/\"",
    "postbuild:flatten": "copyfiles -u 3 \"dist/pacto/browser/**/*\" dist/pacto && copyfiles \"dist/pacto/3rdpartylicenses.txt\" dist/pacto && copyfiles \"dist/pacto/prerendered-routes.json\" dist/pacto && rimraf dist/pacto/browser && rimraf dist/pacto/dist",
    "build:flat": "npm run clean:dist && npm run build:prod && npm run postbuild:flatten",
    "build:flat:gh": "npm run clean:dist && npm run build:prod:gh && npm run postbuild:flatten",
    "copy:flat_to_docs": "copyfiles -u 2 \"dist/pacto/**/*\" docs",
    "build:github_pages": "npm run build:flat:gh && npm run clean:docs && npm run copy:flat_to_docs",
    "e2e": "ng e2e",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^19.2.0",
    "@angular/compiler": "^19.2.0",
    "@angular/core": "^19.2.0",
    "@angular/fire": "^19.1.0",
    "@angular/forms": "^19.2.0",
    "@angular/platform-browser": "^19.2.0",
    "@angular/platform-browser-dynamic": "^19.2.0",
    "@angular/router": "^19.2.0",
    "firebase": "^11.6.1",
    "rxjs": "~7.8.0",
    "sweetalert2": "^11.21.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.9",
    "@angular/cli": "^19.2.9",
    "@angular/compiler-cli": "^19.2.0",
    "@cypress/schematic": "^3.0.0",
    "@types/jasmine": "~5.1.0",
    "angular-cli-ghpages": "^2.0.3",
    "copyfiles": "^2.4.1",
    "cypress": "latest",
    "jasmine-core": "~5.6.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "rimraf": "^6.0.1",
    "typescript": "~5.7.2"
  }
}
